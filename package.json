{
  "name": "coinquest-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 8081 --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview --port 8081 --host 0.0.0.0",
    "backend": "cd backend && python main.py",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    "start:prod": "npm run build && npm run preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "axios": "^1.6.2",
    "lucide-react": "^0.294.0",
    "date-fns": "^2.30.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2"
  }
}