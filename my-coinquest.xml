<?xml version="1.0"?>
<Container version="2">
    <Name>CoinQuest</Name>
    <Repository>taggara/coinquest</Repository>
    <Registry />
    <Network>bridge</Network>
    <MyIP />
    <Shell>bash</Shell>
    <Privileged>false</Privileged>
    <Support>https://github.com/taggara/coinquest/issues</Support>
    <Project>https://github.com/taggara/coinquest</Project>
    <Overview>CoinQuest is a personal finance tracking application built with React and Python FastAPI.</Overview>
    <Category>Productivity: Tools:</Category>
    <WebUI>http://[IP]:[PORT:8081]</WebUI>
    <TemplateURL />
    <Icon>https://i.imgur.com/Ai3kngp.png</Icon>
    <ExtraParams />
    <PostArgs>/bin/bash /appdata/startup.sh</PostArgs>
    <CPUset />
    <DateInstalled>1748299115</DateInstalled>
    <DonateText />
    <DonateLink />
    <Requires />
    <Config Name="WebUI Port" Target="8081" Default="8081" Mode="tcp" Description="Web UI port"
        Type="Port" Display="always" Required="true" Mask="false">8081</Config>
    <Config Name="App Data" Target="/appdata" Default="/mnt/user/appdata/coinquest" Mode="rw"
        Description="Application data directory" Type="Path" Display="always" Required="true"
        Mask="false">/mnt/user/appdata/coinquest/</Config>
    <Config Name="Environment" Target="NODE_ENV" Default="development" Mode=""
        Description="Node environment" Type="Variable" Display="always" Required="true" Mask="false">
        development</Config>
    <Config Name="Database" Target="DATABASE_URL" Default="postgresql://admin:@192.168.4.52:5432/coinquest" Mode=""
        Description="Database connection URL" Type="Variable" Display="always" Required="true"
        Mask="false">postgresql://admin:@192.168.4.52:5432/coinquest</Config>
    <Config Name="GitHub Token" Target="GIT_AUTH_TOKEN" Default="" Mode=""
        Description="GitHub Personal Access Token" Type="Variable" Display="advanced" Required="false"
        Mask="true"></Config>
    <Config Name="Host" Target="HOST" Default="0.0.0.0" Mode="" Description="Host to bind to"
        Type="Variable" Display="advanced" Required="true" Mask="false">0.0.0.0</Config>
    <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="User ID" Type="Variable"
        Display="advanced" Required="true" Mask="false">99</Config>
    <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Group ID" Type="Variable"
        Display="advanced" Required="true" Mask="false">100</Config>
    <TailscaleStateDir />
</Container>