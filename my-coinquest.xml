<?xml version="1.0"?>
<Container version="2">
    <Name>CoinQuest</Name>
    <Repository>taggara/coinquest</Repository>
    <Network>bridge</Network>
    <MyIP />
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <Support>https://github.com/taggara/coinquest/issues</Support>
    <Project>https://github.com/taggara/coinquest</Project>
    <Overview>CoinQuest is a personal finance tracking application built with Expo and React Native. It helps users manage their expenses, track budgets, and analyze spending patterns.</Overview>
    <Category>Productivity: Tools:</Category>
    <WebUI>http://[IP]:[PORT:8099]</WebUI>
    <TemplateURL />
    <Icon>https://i.imgur.com/Ai3kngp.png</Icon>
    <PostArgs />
    <CPUset />
    <DateInstalled>1716700800</DateInstalled>
    <DonateText />
    <DonateLink />
    <Requires />
    <Config Name="WebUI Port" Target="PORT" Default="8099" Mode="tcp" Description="Web UI port"
        Type="Port" Display="always" Required="true" Mask="false">8099</Config>
    <Config Name="App Data" Target="APPDATA_PATH" Default="/mnt/user/appdata/coinquest" Mode="rw"
        Description="Application data directory" Type="Path" Display="always" Required="true"
        Mask="false">/mnt/user/appdata/coinquest</Config>
    <Config Name="Environment" Target="NODE_ENV" Default="production" Mode=""
        Description="Node environment" Type="Variable" Display="always" Required="true" Mask="false">
        production</Config>
    <Config Name="Database" Target="DATABASE_URL" Default="sqlite://appdata/data/local.db" Mode=""
        Description="Database connection URL" Type="Variable" Display="always" Required="true"
        Mask="false">sqlite://appdata/data/local.db</Config>
    <Config Name="GitHub Token" Target="GIT_AUTH_TOKEN" Default="" Mode=""
        Description="GitHub Personal Access Token" Type="Variable" Display="always" Required="true"
        Mask="true" />
    <Config Name="Host" Target="HOST" Default="0.0.0.0" Mode="" Description="Host to bind to"
        Type="Variable" Display="advanced" Required="true" Mask="false">0.0.0.0</Config>
    <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="User ID" Type="Variable"
        Display="advanced" Required="true" Mask="false">99</Config>
    <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="Group ID" Type="Variable"
        Display="advanced" Required="true" Mask="false">100</Config>
    <TailscaleStateDir />
</Container>